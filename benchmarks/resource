#!/usr/bin/env ruby -W0
load File.dirname(__FILE__) + '/utils.rb'

Benchmark.bm(35) do |b|

  b.report 'generating resource' do
    $resource.each do |r|
      $router.resource r
    end
  end

  b.report 'recognizing resource' do
    TIMES.times do
      $app.get($resource.sample)
    end
  end

end
